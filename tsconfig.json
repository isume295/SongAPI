{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "baseUrl": "src",
    "outDir": "dist",
    "sourceMap": false,
    "noImplicitAny": true,
    "target": "es2015",
    "downlevelIteration": true
  },
  "include": ["src/**/*"]
}
